<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_RIGHT" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sqlite_login.Side_barController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <FontAwesomeIconView fill="#deed37" glyphName="CIRCLE" onMouseClicked="#min">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIconView>
      <FontAwesomeIconView fill="#25f471" glyphName="CIRCLE" layoutX="20.0" layoutY="30.0" onMouseClicked="#max">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIconView>
      <FontAwesomeIconView fill="#f85a5a" glyphName="CIRCLE" layoutX="32.0" layoutY="30.0" onMouseClicked="#close">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIconView>
   </children>
</HBox>
